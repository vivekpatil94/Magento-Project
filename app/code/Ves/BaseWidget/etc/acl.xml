<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                
                <resource id="Magento_Backend::basewidget_elements" title="Manage Ves Base Widgets">
                    <resource id="Ves_BaseWidget::base" title="Ves Base Widgets" sortOrder="10">
                        <resource id="Ves_BaseWidget::manage_media" title="Manage Media Files In Admin" sortOrder="10" />
                    </resource>
                </resource>
                
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Ves_BaseWidget::config" title="Config Settings" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>